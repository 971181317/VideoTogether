{
    "name": "Video Together",
    "description": "Watch video together",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "extension.user.js"
            ],
            "all_frames": true
        }
    ],
    "permissions": [
        "storage"
    ]
}